{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:cypress/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "always"]
  },
  "plugins": ["cypress"],
  "ignorePatterns": [
    "cypress/fixtures/*",
    "cypress/plugins/*",
    "cypress/support/*"
  ]
}
